	kill @e[type=ArmorStand,SpawnStand]
	summon ArmorStand $SpawnStand$ {Tags:["SpawnStand"],Invulnerable:1,PersistenceRequired:1,Invisible:1,Marker:1,NoGravity:1}
	
	USE {@e[type=ArmorStand,SpawnStand]}:
		SPtp = 0
		SPpvp = 0
		SPsat = 1
		SPgmd = 1
		SPgam = 0
		SPaff = 7
