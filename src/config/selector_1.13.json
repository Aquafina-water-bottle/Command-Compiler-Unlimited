{   
    "selector_variable_specifiers": ["a", "e", "r", "p", "s"],

    "selector_arguments": [
        "x", "y", "z", "dx", "dy", "dz",
        "distance", "x_rotation", "y_rotation", "level",
        "team", "name", "limit", "tag",
        "type", "sort", "gamemode",
        "nbt", "scores", "advancements"
    ],

    "selector_replacements": {
        "c": "limit",
        "m": "gamemode",
        "s": "sort",
        "adv": "advancements",
        "dist": "distance",
        "lvl": "level",
        "xrot": "x_rotation",
        "yrot": "y_rotation"
    },

    "selector_argument_details": {
        "x": {
            "parse_type": "NUMBER"
        },
        "y": {
            "parse_type": "NUMBER"
        },
        "z": {
            "parse_type": "NUMBER"
        },
        "dx": {
            "parse_type": "NUMBER"
        },
        "dy": {
            "parse_type": "NUMBER"
        },
        "dz": {
            "parse_type": "NUMBER"
        },

        "distance": {
            "parse_type": "NUMBER_RANGE"
        },
        "x_rotation": {
            "parse_type": "NUMBER_RANGE"
        },
        "y_rotation": {
            "parse_type": "NUMBER_RANGE"
        },
        "level": {
            "parse_type": "NUMBER_RANGE"
        },

        "team": {
            "group": "negation",
            "parse_type": "STR",
            "negation": true
        },
        "name": {
            "parse_type": "STR",
            "group": "negation",
            "negation": true
        },
        "limit": {
            "parse_type": "POS_INT"
        },
        "tag": {
            "parse_type": "SHORTCUT_ERROR"
        },

        "type": {
            "parse_type": "ENTITIES",
            "group": "negation",
            "negation": true
        },
        "gamemode": {
            "parse_type": "VALUES",
            "group": "negation",
            "value_replace": {
                "0": "survival",
                "1": "creative",
                "2": "adventure",
                "3": "spectator"
            },
            "values": ["survival", "creative", "adventure", "spectator"],
            "negation": true
        },
        "sort": {
            "parse_type": "VALUES",
            "value_replace": {
                "a": "arbitrary",
                "r": "random",
                "c": "closest",
                "f": "furthest"
            },
            "values": ["arbitrary", "random", "closest", "furthest"]
        },

        "nbt": {
            "parse_type": "SHORTCUT_ERROR",
            "negation": true
        },
        "scores": {
            "parse_type": "SHORTCUT_ERROR"
        },
        "advancements": {
            "parse_type": "ADVANCEMENT_GROUP"
        }
    }
}
