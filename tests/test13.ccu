//<General Definitions
DEF $CMDName$ Randomized_Location
DEF $CMDSizeX$ 16
DEF $CMDSizeY$ 8
DEF $CMDSizeZ$ 3

IMPORT {LIBRARY GETDIR 1.12/General}
IMPORT {LIBRARY 1.12/Math/random.ccu}
//>

//<Options
OPTIONS:
	parseOption true
	commandOption true
	combinerOption true
	filePathFuncOption C:\Users\Austin\Documents\Austin\powder game code\eGO\Server Stuff\1.12.1\world\data\functions\random
	parseChangesOption false
//>


//<General Scoreboard Start
FUNC {Func_GeneralScoreboardStart}:
	ARRAY {GLOBAL ACTIVATE CCU.objectiveAdd(Obj_GeneralScoreboardStart) Obj_GeneralScoreboardStart}:
		Random dummy Location Rand
		
	ScSS Random
	
	FUNC {ACTIVATE CCU.random.init(1;Random) ARGS}:
		DEF $CCUDynamicMin$ false
		DEF $CCUDynamicMax$ true
		DEF $CCURandMin$ 2
		DEF $CCURandMax$ 5
//>

//<General Scoreboard Pulse
FUNC {Func_GeneralScoreboardPulse}:
	FUNC {ACTIVATE CCU.random.main(1;Random;@a[c=1]) ARGS}:
		DEF $CCUDynamicMin$ false
		DEF $CCURandMin$ 2
		DEF $CCUDynamicMax$ true
		
	tellraw @a {"score":{"name":"french_man","objective":"Random"}}
//>

//<General Scoreboard End
FUNC {Func_GeneralScoreboardEnd}:
	CCU.objectiveRemove(Obj_GeneralScoreboardStart)
//>


//<Groups
MFUNC {init MFunc_Init}:
	Func_GeneralScoreboardStart()

MFUNC {main MFunc_Main}:
	Func_GeneralScoreboardPulse()

MFUNC {uninstall Grp_Uninstall}:
	Func_GeneralScoreboardEnd()
//>







